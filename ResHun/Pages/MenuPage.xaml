        <TextBox x:Name="desdes" Text="описание" TextWrapping="Wrap" HorizontalAlignment="Left" Margin="538,178,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.481,1" Width="128" Height="79" BorderBrush="{x:Null}" IsEnabled="False"/>
