        <Image Margin="424,122,112,47" Source="/ResHun;component/img/Rectangle 20.png" Stretch="Fill"/>
        <Image Margin="728,209,26,123" Source="/ResHun;component/img/Vector (1).png" Stretch="Fill"/>
        <Image Margin="28,209,734,123" Source="/ResHun;component/img/Vector (1).png" Stretch="Fill" RenderTransformOrigin="0.5,0.5">
