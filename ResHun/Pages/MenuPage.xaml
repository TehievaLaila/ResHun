        <Label x:Name="Nam" Content="Название" HorizontalAlignment="Left" Margin="123,139,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
        <TextBox x:Name="Descrip" Text="описание" TextWrapping="Wrap"  HorizontalAlignment="Left" Margin="218,178,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.481,1" Height="79" Width="128" BorderBrush="{x:Null}" IsEnabled="False"/>
        <Label x:Name="Mee" Content="тип мяса" HorizontalAlignment="Left" Margin="123,262,0,0" VerticalAlignment="Top"/>
        <Button Content="заказать" Style="{DynamicResource ButtonStyle1}" HorizontalAlignment="Left" Height="34" Margin="223,273,0,0" VerticalAlignment="Top" Width="123" BorderBrush="{x:Null}" Background="#FFE00001" Foreground="White"/>
        <Label x:Name="Pri" Content="цена" HorizontalAlignment="Left" Height="33" Margin="124,283,0,0" VerticalAlignment="Top" Background="{x:Null}" FontSize="14" FontWeight="Bold"/>
